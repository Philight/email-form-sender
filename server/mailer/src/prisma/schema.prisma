generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model Email {
  id      String @id @default(auto()) @map("_id") @db.ObjectId
  email String 
  emailcc String 
  emailto String 
  body String
  image String? @default("default.png")

  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}